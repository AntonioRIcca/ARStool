{
	"QMainWindow":[
		{
			"tittle":"ENEA ARS tool",
			"icon":":/images/LogoENEA_eng_200x80.png",
			"frameless": true,
			"transluscentBg": true,
			"sizeGrip": "sizeGrip",
			"navigation":[
				{
					"minimize":"minimizeBtn",
					"close": "closeBtn",
					"restore":[
						{
							"buttonName": "restoreBtn",
							"normalIcon": ":/icons/icons/maximize.png",
							"maximizedIcon": ":/icons/icons/unselect.png"
						}
					],
					"moveWindow": "headerContainer",
					"tittleBar": "headerContainer"
				}
			]
		}
	],

	"QCustomSlideMenu": [
		{
			"name": "leftMenuContainer",
			"defaultSize": [{
				"width": 53,
				"height": "parent"
			}],
			"collapsedSize": [{
				"width": 53,
				"height": "parent"
			}],
			"expandedSize": [{
				"width": 130,
				"height": "parent"
			}],
			"toggleButton": [{
				"buttonName": "menuBtn",
				"icons": [{
					"whenMenuIsCollapsed": ":/icons/icons/menu.png",
					"whenMenuIsExpanded": ":/icons/icons/back.png"
				}]
			}],
			"menuTransitionAnimation": [{
				"animationDuration": 500,
				"animationEasingCurve": "Linear"
			}]
		},
		{
			"name": "centerMenuContainer",
			"defaultSize": [{
				"width": 0,
				"height": "parent"
			}],
			"collapsedSize": [{
				"width": 0,
				"height": "parent"
			}],
			"expandedSize": [{
				"width": 200,
				"height": "parent"
			}],
			"menuTransitionAnimation": [{
				"animationDuration": 500,
				"animationEasingCurve": "Linear"
			}]
		},
		{
			"name": "rightMenuContainer",
			"defaultSize": [{
				"width": 0,
				"height": "parent"
			}],
			"collapsedSize": [{
				"width": 0,
				"height": "parent"
			}],
			"expandedSize": [{
				"width": 200,
				"height": "parent"
			}],
			"menuTransitionAnimation": [{
				"animationDuration": 500,
				"animationEasingCurve": "Linear"
			}]
		},
		{
			"name": "popupNotificationContainer",
			"defaultSize": [{
				"width": 0,
				"height": "auto"
			}],
			"collapsedSize": [{
				"width": 0,
				"height": "auto"
			}],
			"expandedSize": [{
				"width": 500,
				"height": 100
			}],
			"menuTransitionAnimation": [{
				"animationDuration": 500,
				"animationEasingCurve": "Linear"
			}],
			"floatPosition": [
				{
					"relativeTo": "centralwidget",
					"position": "center-center",
					"shadow":[
						{
							"color": "#000",
							"blurRadius": 20,
							"xOffset": 0,
							"yOffset": 0
						}
					]
				}
			],
			"toggleButton": [
				{
					"buttonName": "notificationBtn"
				}
			]
		}
	],
	"QStackedWidget":[
		{
			"name":"mainPages",
			"navigation":[
				{
					"navigationButtons":[
						{
							"homeBtn": "page_6",
							"dataBtn": "page_7",
							"reportBtn": "page_8"
						}
					]
				}
			]

		},
		{
			"name":"centerMenuPages",
			"navigation":[
				{
					"navigationButtons":[
						{
							"settingsBtn": "page",
							"infoBtn": "page_2",
							"helpBtn": "page_3"
						}
					]
				}
			]

		},
		{
			"name":"rightMenuPages",
			"navigation":[
				{
					"navigationButtons":[
						{
							"profileMenuBtn": "page_4",
							"moreMenuBtn": "page_5"
						}
					]
				}
			]

		}
	],
	"QPushButtonGroup": [
		{
			"Buttons": [
				"homeBtn",
				"dataBtn",
				"reportBtn"
			],
			"Style":[
				{
					"Active": "background-color: #1f232a;",
					"NotActive": ""
				}
			]
		},
		{
			"Buttons": [
				"settingsBtn",
				"infoBtn",
				"helpBtn"
			],
			"Style":[
				{
					"Active": "background-color: #2c313c;",
					"NotActive": ""
				}
			]
		}
	]
}